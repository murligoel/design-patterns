+package com.practice;
+
+interface IDR {
+    public void unlockPdf(String fileName, String password);
+}
+
+class RealDocumentReader implements IDR {
+
+    @Override
+    public void unlockPdf(String fileName, String password) {
+        System.out.println("Unlocking file " + fileName);
+    }
+}
+
+class User {
+    public String username;
+    public boolean isPremium;
+
+    public User(String username, boolean isPremium) {
+        this.username = username;
+        this.isPremium = isPremium;
+    }
+}
+
+class DocumentProxy implements IDR {
+    private RealDocumentReader realDocumentReader;
+    private User user;
+    public DocumentProxy( User user) {
+        this.realDocumentReader = new RealDocumentReader();
+        this.user = user;
+    }
+
+    @Override
+    public void unlockPdf(String fileName, String password) {
+       if(user.isPremium) {
+           realDocumentReader.unlockPdf(fileName, password);
+           return;
+       }
+       System.out.println("User is not premium");
+    }
+}
+public class PP {
+
+    public static void main(String[] args) {
+        User user = new User("VP-murli", false);
+        User user2 = new User("VP-murli-jqjh", true);
+
+        IDR realDocumentReader = new DocumentProxy(user);
+        realDocumentReader.unlockPdf("file.txt", "password");
+
+        realDocumentReader = new DocumentProxy(user2);
+        realDocumentReader.unlockPdf("file2.txt", "password");
+    }
+}
