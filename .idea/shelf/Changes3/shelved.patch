+package com.practice;
+
+import java.util.ArrayList;
+import java.util.List;
+
+public class IphoneSO implements SO{
+
+    List<NAO> li = new ArrayList<>();
+
+    int stockCount = 0;
+
+    @Override
+    public void add(NAO nao) {
+        li.add(nao);
+    }
+
+    @Override
+    public void remove(NAO nao) {
+        li.remove(nao);
+    }
+
+    @Override
+    public void notifySubscribers() {
+        for(NAO nao : li){
+            nao.update();
+        }
+    }
+
+    @Override
+    public void setStockCount(int stockCount) {
+        if(stockCount > 0){
+            notifySubscribers();
+        }
+        this.stockCount = this.stockCount + stockCount;
+    }
+
+    @Override
+    public int getStockCount() {
+        return this.stockCount;
+    }
+}
