@@ -0,0 +1,39 @@
+package com.practice;
+
+import java.util.ArrayList;
+import java.util.List;
+
+public class IphoneObservableImpl implements StocksObservable{
+
+    List<NotificationAlertObserver> observers = new ArrayList<>();
+
+    int stockCount = 0;
+
+    @Override
+    public void add(NotificationAlertObserver o) {
+        observers.add(o);
+    }
+
+    @Override
+    public void remove(NotificationAlertObserver o) {
+        observers.remove(o);
+    }
+
+    @Override
+    public void notifySubscribers() {
+        for(NotificationAlertObserver o : observers){
+            o.update();
+        }
+    }
+
+    @Override
+    public void setStockCount(int stockCount) {
+        this.stockCount = this.stockCount + stockCount;
+        notifySubscribers();
+    }
+
+    @Override
+    public int getStockCount() {
+        return this.stockCount;
+    }
+}
